{
  "name": "bornfight-shopify-starter",
  "version": "1",
  "private": true,
  "author": "Bornfight",
  "description": "This is a start kit for Shopify projects by Bornfight agency. Kit is compatible with Shopify Slate and based on Shopify starter theme. SCSS structure is Bornfights project starter kit standard.",
  "keywords": [
    "shopify",
    "theme"
  ],
  "bugs": "https://github.com/degordian/bornfight-shopify-starter/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/degordian/bornfight-shopify-starter.git"
  },
  "devDependencies": {
    "@shopify/slate-tools": "1.0.0-beta.14",
    "babel-preset-shopify": "^16.2.0",
    "eslint-plugin-shopify": "^19.0.0",
    "stylelint-config-prettier": "^3.0.4",
    "stylelint-config-shopify": "^5.0.0"
  },
  "dependencies": {
    "@degordian/sassbox": "^0.4.0",
    "@shopify/theme-a11y": "^1.0.1",
    "@shopify/theme-addresses": "^3.0.3",
    "@shopify/theme-cart": "^1.0.0-alpha.7",
    "@shopify/theme-currency": "^1.0.0-alpha.7",
    "@shopify/theme-images": "^3.0.3",
    "@shopify/theme-rte": "^1.0.0-alpha.7",
    "@shopify/theme-sections": "^3.0.3",
    "@shopify/theme-variants": "^1.0.0-alpha.8",
    "jquery": "^3.2.1",
    "lazysizes": "^4.1.1",
    "lodash-es": "^4.17.14",
    "normalize.css": "^7.0.0"
  },
  "scripts": {
    "start": "slate-tools start",
